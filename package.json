    "start:tailwind": "npx tailwindcss -i ./src/main.css -o ./site/css/main.css --watch",
    "build:tailwind": "npx tailwindcss -i ./src/main.css -o ./site/css/main.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brave/config-attestation.git"
